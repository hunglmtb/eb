	<parameter name="facility_id" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT
x.FACILITY_NAME,
x.WELL_NAME,
x.THEOR_METHOD,
x.OCCUR_DATE,
x.GOR,
x.SW,
x.ACTIVE_HRS,
x1.EU_DAY_GRS_VOL AS OIL_ALLOC, y1.EU_DAY_GRS_VOL AS OIL_THEOR,
x2.EU_DAY_GRS_VOL AS GAS_ALLOC, y2.EU_DAY_GRS_VOL AS GAS_THEOR,
x3.EU_DAY_GRS_VOL AS WATER_ALLOC, y3.EU_DAY_GRS_VOL AS WATER_THEOR
FROM
(SELECT
f.NAME AS FACILITY_NAME,
eu.ID AS EU_ID,
eu.NAME AS WELL_NAME,
eu.TEST_METHOD AS THEOR_METHOD,
udv.OCCUR_DATE AS OCCUR_DATE,
MAX(udv.ACTIVE_HRS) AS ACTIVE_HRS,
MAX(GOR) AS GOR,
MAX(SW) AS SW 
FROM energy_unit AS eu
INNER JOIN energy_unit_day_value AS udv
ON eu.ID = udv.EU_ID
INNER JOIN facility AS f
ON eu.FACILITY_ID = f.ID
INNER JOIN code_event_type AS et
ON eu.EVENT_TYPE=et.ID
WHERE f.ID=1
AND et.ID=1
GROUP BY eu.ID, eu.NAME, eu.TEST_METHOD, udv.OCCUR_DATE) AS x
INNER JOIN energy_unit_day_alloc AS x1
ON x.EU_ID = x1.EU_ID AND x.OCCUR_DATE = x1.OCCUR_DATE AND x1.FLOW_PHASE = 1
INNER JOIN energy_unit_day_alloc AS x2
ON x.EU_ID = x2.EU_ID AND x.OCCUR_DATE = x2.OCCUR_DATE AND x2.FLOW_PHASE = 2
INNER JOIN energy_unit_day_alloc AS x3
ON x.EU_ID = x3.EU_ID AND x.OCCUR_DATE = x3.OCCUR_DATE AND x3.FLOW_PHASE = 3
INNER JOIN energy_unit_day_theor AS y1
ON x.EU_ID = y1.EU_ID AND y1.OCCUR_DATE = y1.OCCUR_DATE AND y1.FLOW_PHASE = 1
INNER JOIN energy_unit_day_theor AS y2
ON x.EU_ID = y2.EU_ID AND y2.OCCUR_DATE = y2.OCCUR_DATE AND y2.FLOW_PHASE = 2
INNER JOIN energy_unit_day_theor AS y3
ON x.EU_ID = y3.EU_ID AND y3.OCCUR_DATE = y3.OCCUR_DATE AND y3.FLOW_PHASE = 3
GROUP BY x.FACILITY_NAME,
x.WELL_NAME,
x.THEOR_METHOD,
x.OCCUR_DATE,
x.GOR,
x.SW,
x.ACTIVE_HRS]]>
	</queryString>
