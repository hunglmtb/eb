	<parameter name="facility_id" class="java.lang.Integer"/>
	<parameter name="begin_date" class="java.sql.Date"/>
	<parameter name="end_date" class="java.sql.Date"/>
	<queryString language="SQL">
		<![CDATA[select  x.GROUP_NAME,x.WELL_NAME,x.THEOR_METHOD,x.OCCUR_DATE,
 x.GOR,x.SW,x.FACILITY_NAME,
 (select max(ACTIVE_HRS) from ENERGY_UNIT_DAY_VALUE v where v.eu_id=x.EU_ID and v.OCCUR_DATE=x.OCCUR_DATE) ACTIVE_HRS,
 x1.EU_DAY_GRS_VOL OIL_ALLOC, y1.EU_DAY_GRS_VOL OIL_THEOR,
 x2.EU_DAY_GRS_VOL GAS_ALLOC, y2.EU_DAY_GRS_VOL GAS_THEOR,
 x3.EU_DAY_GRS_VOL WATER_ALLOC, y3.EU_DAY_GRS_VOL WATER_THEOR
from
(select distinct g.NAME GROUP_NAME, a.name WELL_NAME, b.EU_ID,f.NAME as FACILITY_NAME,
 t.CODE THEOR_METHOD,
 b.OCCUR_DATE,
 null GOR,null SW
from 
 facility f,
 energy_unit a 
 left join code_theoretical_method t on a.TEST_METHOD=t.ID
 left join energy_unit_group g on g.ID=a.EU_GROUP_ID,
 ENERGY_UNIT_DAY_ALLOC b
where f.id=$P{facility_id}
and b.alloc_type=1
and a.EVENT_TYPE=1
 and b.eu_id=a.id
 and a.facility_id=f.id
 and b.OCCUR_DATE>=$P{begin_date}
 and b.OCCUR_DATE<=$P{end_date}
 and b.EU_DAY_GRS_VOL is not null
group by a.EU_GROUP_ID,b.EU_ID,b.OCCUR_DATE
) x
left join ENERGY_UNIT_DAY_ALLOC x1 on x1.eu_id=x.EU_ID and x1.alloc_type=1 and x1.OCCUR_DATE=x.OCCUR_DATE and x1.FLOW_PHASE=1
left join ENERGY_UNIT_DAY_ALLOC x2 on x2.eu_id=x.EU_ID and x2.alloc_type=1 and x2.OCCUR_DATE=x.OCCUR_DATE and x2.FLOW_PHASE=2
left join ENERGY_UNIT_DAY_ALLOC x3 on x3.eu_id=x.EU_ID and x3.alloc_type=1 and x3.OCCUR_DATE=x.OCCUR_DATE and x3.FLOW_PHASE=3

left join ENERGY_UNIT_DAY_THEOR y1 on y1.eu_id=x.EU_ID and y1.OCCUR_DATE=x.OCCUR_DATE and y1.FLOW_PHASE=1
left join ENERGY_UNIT_DAY_THEOR y2 on y2.eu_id=x.EU_ID and y2.OCCUR_DATE=x.OCCUR_DATE and y2.FLOW_PHASE=2
left join ENERGY_UNIT_DAY_THEOR y3 on y3.eu_id=x.EU_ID and y3.OCCUR_DATE=x.OCCUR_DATE and y3.FLOW_PHASE=3]]>
	</queryString>
