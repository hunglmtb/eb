	<queryString language="SQL">
		<![CDATA[SELECT c.name category, b.name equipment, ifnull(a.eqp_online_hours,0) active_hrs, case when a.EQP_OFFLINE_HOURS>0 || ifnull(a.eqp_online_hours,0)=0 then 'Offline' when a.EQP_ONLINE_HOURS>0 then 'Online' else null end status, ifnull(d.name,'Unknown') DT_reason, a.eqp_note commnents
FROM equipment_day_value a
INNER JOIN equipment b ON a.equipment_id=b.id
INNER JOIN code_equipment_type c ON b.equipment_type=c.id
LEFT JOIN code_eqp_offline_reason d ON a.offline_reason_code=d.id
WHERE b.facility_id=$P{facility_id}
AND a.occur_date=$P{date}
ORDER BY c.id]]>
