	<queryString language="SQL">
		<![CDATA[SELECT a.occur_date, b.name storage, c.name product, 
  case f.UOM_TYPE when 2 then 'Bbl' else 'm3' end uom, 
  a.storage_grs_vol, null max_vol, a.storage_grs_vol-closingPrevDayStorage(a.occur_date, storage_id) grs_diff_prev_day 
FROM storage_day_value a, facility f, 
STORAGE b LEFT JOIN code_product_type c ON c.id = b.product
WHERE a.occur_date=$P{date}
AND b.facility_id=$P{facility_id}
AND b.id = a.storage_id
AND b.FACILITY_ID = f.ID]]>
	</queryString>
