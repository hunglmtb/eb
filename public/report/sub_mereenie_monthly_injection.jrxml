	<queryString language="SQL">
		<![CDATA[select eu_id,res_id,well_name,res_name, 
(select case h.status when 1 then 'ONLINE' else 'OFFLINE' end well_status from energy_unit_history h where h.eu_id=x.eu_id and date_format(h.CHANGED_DATE,'%Y/%m')<=date_format($P{startTime},'%Y/%m') order by h.CHANGED_DATE desc  limit 1 ) well_status,
active_days,round(v_gas/1000,2) v_gas,round(month_gas/1000,2) month_gas,
round(case when x.active_days=0 then 0 else x.v_gas/1000/x.active_days end,2) day_gas
from(
SELECT a.eu_id,e.id res_id,b.name well_name,e.name res_name,sum(c.GRS_VOL) v_gas,
(select round(sum(f.active_hrs)/24,2) active_days from energy_unit_day_fdc_value f 
where f.eu_id=a.eu_id and date_format(f.occur_date,'%Y/%m/01')=date_format($P{startTime},'%Y/%m/01') and f.FLOW_PHASE=2 and f.event_type=2) active_days,
sum(case when date_format(c.occur_date,'%Y/%m/01')=date_format($P{startTime},'%Y/%m/01') then c.GRS_VOL else 0 end) month_gas
FROM `well_comp_interval_perf` a, energy_unit b, well_comp_interval_perf_day_alloc c, reservoir_formation d, reservoir e, well_comp_interval i
WHERE a.eu_id=b.id and b.facility_id in (18,19)
and a.comp_interval_id=i.id and i.RESERVOIR_FORMATION=d.id and d.reservoir_id=e.id
and c.comp_interval_perf_id=a.id
and date_format(c.occur_date,'%Y/%m/01')<=date_format($P{startTime},'%Y/%m/01')
and c.FLOW_PHASE=2 and c.event_type=2
group by res_id,eu_id
) x
group by x.res_id,x.eu_id order by x.res_name,x.well_name]]>
	</queryString>
